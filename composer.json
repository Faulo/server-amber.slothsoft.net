{
	"name" : "slothsoft/amber.slothsoft.net",
	"type" : "project",
	"description" : "amber.slothsoft.net",
	"license" : "MIT",
	"keywords" : [
		"slothsoft",
		"farah-module",
		"farah-server"
	],
	"autoload" : {
		"files" : [
			"src/bootstrap.php"
		],
		"psr-4" : {
			"Slothsoft\\Server\\Amber\\" : "src"
		}
	},
	"require" : {
		"slothsoft/amber" : "*",
		"slothsoft/chat" : "*",
		"slothsoft/core" : "^1.11",
		"slothsoft/farah" : "^1.12"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^8.5"
	},
	"autoload-dev" : {
		"classmap" : [
			"tests/"
		],
		"psr-4" : {
			"Slothsoft\\Server\\Amber\\" : "tests"
		}
	},
	"scripts" : {
		"farah-asset" : "@php vendor/slothsoft/farah/scripts/farah-asset.php",
		"farah-page" : "@php vendor/slothsoft/farah/scripts/farah-page.php"
	},
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"homepage" : "http://amber.slothsoft.net"
}
